{
    "hosting": {
        "headers": [
            {
                "source": "/**",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "no-cache, no-store, must-revalidate"
                    }
                ]
            }
        ],
        "rewrites": [
            {
                "source": "/",
                "run": {
                    "serviceId": "rssfeedreader",
                    "region": "us-west1"
                }
            }
        ]
    }
}
